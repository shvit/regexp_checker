  var registerGaTopMenuRight = function (items)  {

    var menuNav = document.querySelectorAll('.segmentNavGa')


    menuNav.forEach(
      function (nav) {
        nav.children.forEach(
          function (link) {
            link.addEventListener('click', function () {
              var segment = RTK_UTILS ? RTK_UTILS.currentSegment : 'b2c'
              if(segment === 'b2c') return
              dataLayer.push({
                  'event': segment,
                  'type': 'header_nav',
                  'value': link.textContent
                }
              )
            })
          }
        )
      }
    )

  }
  window.addEventListener('load', registerGaTopMenuRight)
</script></div>
      
