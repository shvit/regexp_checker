	/** Функция для получения значения в cookie */
	function getCookie(name) {
		var matches = document.cookie.match(new RegExp(
			"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
		));
		return matches ? decodeURIComponent(matches[1]) : undefined;
	}
	window.getCookie = getCookie

	/** Функция для получения значения в параметрах URL */
	function getParam(name) {
		return (new URLSearchParams(location.search)).get(name)
	}
	window.getParam = getParam

	/**
		* Get DOM-element when him is appended
		*/
	function waitLoaded(selector, callback) {
		var call = 0,
			condition = function () { return call > 50 || document.querySelector(selector) },
			promise = function () { if (condition()) { callback(document.querySelector(selector)); } else { var timer = setInterval(function () { if (condition()) { clearInterval(timer); callback(document.querySelector(selector)); } else { call++; } }, 250); } };
		promise();
	};
	window.waitLoaded = waitLoaded

	/**
		* Check current GEO by regions array
		*/
	function checkGeo(regionIds) {
		if (!regionIds || !Array.isArray(regionIds)) return console.log('Не найден список regionIds');
		return regionIds.indexOf(parseInt(geoData.regionId)) !== -1
	}
	window.checkGeo = checkGeo


	// Определяет тач-девайс
	function isTouchDevice() { return "ontouchstart" in document.documentElement }

	// Добавляет CSS-класс
	function addClass(e, t) { e.classList ? e.classList.add(t) : e.className += " " + t }

	// Пушит в метрику данные
	function linkToConstructorClicked(e, slide) {
		window.RTK_UTILS_METRICS({
			event: 'constructor_show',
			detail: {
				offerId: slide.offerId,
				offerName: slide.offerName || slide.title,
				constructorStep: 'settings',
			},
			referrer: window.location.href
		},
			{
				isNew: true,
				key: 'constructorSEO'
			}
		)
	}

	// Сигнализирует что это тач-девайс
	if (isTouchDevice()) addClass(document.body, 'is-touch');

	(function () {
		if (decodeURI(window.location.href).match(/(?!&)utm_[^=]*=[^&]*(?=&)/g)) {
			window.sessionStorage.setItem('utm_label', JSON.stringify(
				decodeURI(window.location.href).match(/(?!&)utm_[^=]*=[^&]*(?=&)/g).reduce(function (acc, param) {
					var separator = param.split('=')
					acc[separator[0]] = separator[1]
					return acc
				}, {})
			))
		}
	})()

	// Регионозависимое уведомление о смене региона
	window.RTK_USE_GEO_POPUP = {
		onShow: function (geo) { setTimeout(function () { if (window.RTK_USE_GEO_POPUP.current && window.RTK_USE_GEO_POPUP.current === "region-notification-dark") { window.RTK_UTILS.metrics({ event: 'b2c', type: 'view_geo_popup', value: geo.regionEng }) } }, 5) },
		dark: [].concat(
			//mrfSeveroZapad,
			//mrfCenter,
			//mrfMoscow,
			//mrfUral,
			//mrfVolga,
			//mrfSibir,
			//mrfDV,
			//mrfYug
		),
		old: [].concat(
			mrfSeveroZapad,
			mrfCenter,
			mrfMoscow,
			mrfUral,
			mrfVolga,
			mrfSibir,
			mrfDV,
			mrfYug
		)
	}

	// window.RTK_VALIDATE_FIELDS_SUGGEST_ADDRESS = ['house']
	// window.RTK_VALIDATE_FIELDS_SUGGEST_ADDRESS = window.checkGeo(window.mrfSibir) ? ['house', 'flat'] : ['house']
