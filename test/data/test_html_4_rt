<script data-ssr>
window.acceptCookie = () => {
	document.getElementById('cookie-notification').style.display = 'none'
	document.cookie = '_ca=1;path=/;domain=.rt.ru;max-age=2592000;'
}
window.isCookieNotificationVisible = () => {
	const visible = !+window.getCookie('_ca')
	const isNotTest = typeof window.getParam === 'function' && window.getParam('test') !== '1'
	if (visible && isNotTest) {
		setTimeout(() => {
			document.getElementById('cookie-notification').style.display = 'block'
		}, 1)
	}
	return visible
}
</script>

