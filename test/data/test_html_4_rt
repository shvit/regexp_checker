// RTRU-15179 Разметка главной страницы
(function () {
  const mainShowcaseSelector = '[about="/main_noindex"] .web-component'
  function isUserAuth() {
    let status = false
    try {
      if (window.sessionStorage.getItem('is_user_auth')==='1') status = true
    } catch (err) { }
    return status
  }
  function markActionForHomePage(eventName, val) {
    const pushData = { [eventName]: val }
    if (isUserAuth()) pushData[eventName] = { [val]: 'rostelecom' } 
    const data = { b2c_head_click: { [window._getGaClientId()]: pushData } }
    if (window.ym) window.ym(window.RTK_YANDEX_COUNTER_IDS.B2C, 'params', data)
  }

  // Tabs
  waitLoaded(`${mainShowcaseSelector} .rt-tabs`, el => {
    if (!el) return
    el.querySelectorAll('.rt-tabs-nav-v2_item button').forEach(tab => tab.addEventListener('click', e => markActionForHomePage('main_tab', tab.innerText)))
  })

  // Tariff cards
  waitLoaded(`${mainShowcaseSelector} .rtk-fo__case .rt-tariff`, card => {
    if (!card) return
    document.querySelectorAll(`${mainShowcaseSelector} .rtk-fo__case .rt-tariff`).forEach(card => {
      card.querySelectorAll('.rt-font-right button, .rt-tariff__link')
        .forEach(btn => btn.addEventListener('click', e =>
          markActionForHomePage('card_click', card.querySelector('.rt-tariff__title').innerText)
        ))
    })
  })

  // Payment
  waitLoaded('.web-component .payment-form', form => {
    if (!form) return
    const cmp = form.__vue__
    form.querySelector('button').addEventListener('click', e =>
      markActionForHomePage('payment', cmp.account && cmp.sum ? 'Перейти к оплате' : 'error'))
  })
  waitLoaded('.web-component .payment-search', btn => {
    if (!btn) return
    btn.addEventListener('click', e =>
      markActionForHomePage('payment_search', 'Поиск платежа')
    )
  })
  waitLoaded('.web-component .payment-other', btn => {
    if (!btn) return
    btn.addEventListener('click', e =>
      markActionForHomePage('payment_other', 'Оплата услуг других организаций')
    )
  })

  // Popular services
  waitLoaded('.web-component .popular-services .popular-services__cards', cards => {
    if (!cards) return
    cards.querySelectorAll('a').forEach(link => {
      if (!link.parentElement) return // Avoid carousel's slots resolver effect
      link.addEventListener('click', e =>
        markActionForHomePage('popular_services', link.querySelector('.rt-card-image__label')?.innerText)
      )
    })
  })

  // Product banner
  /*waitLoaded('.web-component .productBanner', banner => {
    if (!banner) return
    banner.querySelector('a').addEventListener('click', e =>
      markActionForHomePage('banner_product', banner.querySelector('.productBanner__title')?.innerText)
    )
  })*/
  waitLoaded('.web-component .productBanner a', bannerLink => {
    if (!bannerLink)return
    bannerLink.addEventListener('click', e =>
      markActionForHomePage('banner_product', bannerLink.parentElement.querySelector('h3')?.innerText)
    )
  })

  // Special services
  waitLoaded('.web-component .special-services .special-services__cards', cards => {
    if (!cards) return
    cards.querySelectorAll('a').forEach(link => {
      if (!link.parentElement) return // Avoid carousel's slots resolver effect
      link.addEventListener('click', e =>
        markActionForHomePage('special_offers', link.querySelector('.rt-card-image__label')?.innerText)
      )
    })
  })

  // Blog announces
  waitLoaded('#announce .announce', a => {
    if (!a) return
    function getAnnounceTitle(link) {
      const textEl = link.classList.contains('announce__weekly__cover') ? link.parentElement.querySelector('.announce__weekly__info__title') : link
      return textEl.innerText
    }
    a.querySelectorAll('a').forEach(link => {
      if (!link.href) return
      link.addEventListener('click', e =>
        markActionForHomePage('blog_click', getAnnounceTitle(link))
      )
    })
  })
})()
</script>
</div>
      
  </div>
