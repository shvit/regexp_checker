    function getPhone(data){
        var
            phoneResult,
            utmData=getUtmData(),
            checkSeo=function(data, phone){if(utmData && Object.keys(utmData).length){
                var
                    invalids=[],
                    exclude={};
                Object.keys(data).forEach(function(key){
                    var notFinded=utmData[key]!==data[key];
                    if(data[key].indexOf('!')===0){
                        var excludeVal=data[key].replace('!','');
                        notFinded=utmData[key]===excludeVal;
                        exclude[key]=excludeVal;
                    }
                    if(data[key].indexOf('*')!==-1)notFinded=utmData[key].indexOf(data[key].replace('*',''))===-1;
                    if((!exclude[key] && !utmData[key]) || notFinded)invalids.push(key)
                })
                if(!invalids.length) phoneResult=phone.replace('-','').replace('(','').replace(')','');
            }};
        data.forEach(function(rule){checkSeo(rule[0],rule[1])});
        return phoneResult || data[0][1];
    }
    function getUtmData(){
        if (typeof window.getCookie === 'function') {
            return JSON.parse(window.getCookie('utm_data') || '{}')
        } else {
            console.error('Global getCookie func no finded')
        }
    }
    function convertStrToTel(str){return 'tel:'+str.replace(/\s/g,'')}
    function getLinkLabel(link){return '<i class='+link.icon+'></i><span>'+link.title+'</span>'}
    function setHeaderPhone(phone,desc){
        if(!phone) return;
        window.headerPhone.title=phone;
        window.headerPhone.url=convertStrToTel(phone);
        if(desc)window.headerPhone.desc=desc;
    }
    function setListPhone(phone,desc,key){
        if(!phone) return;
        var phoneItem=window.phonesList[key||0];
        if(!phoneItem) return;
        phoneItem.title=phone;
        phoneItem.url=convertStrToTel(phone);
        if(desc)phoneItem.desc=desc;
    }
    function setBadges(links){
        var
            getBadge=function(badge){return document.getElementById(badge);},
            badgeApple=getBadge('apple-store'),
            badgeGoogle=getBadge('google-store'),
            badgeHuawei=getBadge('app-gallery');
        if(badgeApple&&links.apple)badgeApple.href=links.apple;
        if(badgeGoogle&&links.google)badgeGoogle.href=links.google;
        if(badgeHuawei)badgeHuawei.style.display='none';
    }
    function initPhones(){
        if(isCustomPhones) return;
        if(isOnlime){
            setBadges(appsLinks);
            setListPhone('8 800 707 8000','Подключить');
            setListPhone(onlimeSupportPhone,null,1);
            // setListPhone(getPhone(phonesOnlime));
        }else if(isSZ){
            setListPhone(getPhone(phonesSZ));
            setListPhone(mainPhone,null,1);
        }else if(isUral && isNotLP){
            setListPhone(getPhone(phonesUral));
            setListPhone(uralSupportPhone,null,1);
        }else if(isMosoblast){
            setListPhone('8 800 707 8058','Подключить',0);
            setListPhone(federalSupportPhone,null,1);
        }else if(isCenter){
            setListPhone(getPhone(phonesCenter),'Подключить',0)
            setListPhone(federalSupportPhone,null,1)
        }else if(isNewRule){
            setListPhone(window.checkGeo(window.mrfYug) ? '8 800 301 38 60' : '8 800 301 84 30');
        }else{
            window.phonesList.splice(0,1);
            window.phonesList[0].desc=phoneCombineDesc;
        }
    }
    initPhones();
</script>
</noindex>
</div>
      
  </div>
<div id="block-b2crazmetkaglavnoystranicy" class="settings-tray-editable block block-block-content block-block-contentab947991-7a72-4d4d-adce-b8618a82792a" data-drupal-settingstray="editable">
  
    
      
