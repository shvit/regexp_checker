<article role="article" about="/node/6869">

  
  

  <div>
    
      <div class="field field--name-field-geo-content field--type-entity-reference-revisions field--label-hidden field__items">
              <div class="field__item">  <div class="paragraph paragraph--type--geo-paragraph paragraph--view-mode--default">
          
            <div class="clearfix text-formatted field field--name-field-body field--type-text-long field--label-hidden field__item"><div class="wc-inline">
	<omni-chat :regions-options="global.REGIONS_OPTIONS" />
</div>

<script>
	var REGIONS_OPTIONS = [
