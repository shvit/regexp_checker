<script>
window.waitLoaded('.header', function (headerEl) {
  if (!headerEl || window.RTK_HEADER.isElkAuth) return
  window.waitLoaded('#userPanelComponent', function (el) {
    if (!el) return
    const cmp = el.__vue__
    if (!cmp) return
    window.RTK_AUTH_LINK=cmp.ssoLink
    const manager = cmp.init()
    const blockName = 'short-name-block-id'
    const checkerDelay = 3000
    manager.subscribe('onAuthenticated', function (isAuth) {
      window.RTK_EVENT_BUS.setData('userAuthorized', isAuth)
      if (isAuth) {
        manager.renderShortNameWidget(blockName)
        window.RTK_AUTH_API=manager.api
      }
      //console.log('onAuthenticated', isAuth, cmp, manager)
      cmp.onAuthenticated(isAuth)
    })
    // проверяем валидность куки
    // const refreshToken=()=>window.location.href='//start.rt.ru'
    // setTimeout(() => {
    //   //console.log('reAuthenticate', manager.isAuthenticated, !!document.getElementById(blockName).innerHTML, cmp, manager)
    //   if (manager.isAuthenticated && !document.getElementById(blockName).innerHTML) {
    //     manager.api('/v2/lego-session').then(resp=>{
    //       if(!resp.data.role){
    //         refreshToken()
    //       }
    //     }).catch(() => {
    //       refreshToken()
    //     })
    //   }
    // }, checkerDelay)
  })
})
</script>
