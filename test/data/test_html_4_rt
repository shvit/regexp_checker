const counters={b2c:'87747669',b2b:'46297389',b2o:'87747838'}
const counterId=counters[window.RTK_SEGMENT]||console.error('Ошибка получения счетчика аналитики')
const ymSafePush=(...data)=>{
  if(window.RTK_SEGMENT!=='b2b')ym(counters['b2b'], ...data) // b2b=all(omg) @see RTRU-15163
  ym(counterId,...data)
}
const promoBlockSelector='.rt-header-menu-promo-block'
// const isRtkUser=()=>+window.sessionStorage.getItem('is_user_auth')?'rostelecom':null // @TMP
if(typeof ym!=='function') ym=(...a)=>console.log('YM.debug',a,JSON.stringify(a))

function pushGA(data) {
  if (!data.event || data.event === 'b2c') return
  if (!window.dataLayer) window.dataLayer = []
  window.dataLayer.push(data)
}
const promoBlockPush=data=>{
  const events={
    event:window.RTK_SEGMENT,
    type:'nav',
    value:'element_click',
    label:'banner_nav',
    content:data.title||'Заголовок не определен',
    num:'NA'
  }
  ymSafePush('reachGoal','events',{events})
  if(window.RTK_SEGMENT === 'b2c'){
    const authorized=isRtkUser()
    const params={[`${window.RTK_SEGMENT}_banner_nav`]:{}}
    params[`${window.RTK_SEGMENT}_banner_nav`][_getGaClientId()]=(authorized)?{[data.title]:authorized}:data.title
    ymSafePush('params',params)
  }
}

if (window.RTK_SEGMENT === 'b2b') {
  // RTRUB2B-7204
  document.addEventListener('click', function(e) {
    if(e.target) {
      const isCurrentNodeIsA = e.target.classList.contains('main-nav__link')
      const isParentNodeIsA =  e.target?.parentNode.classList.contains('main-nav__link')
      if (isCurrentNodeIsA || isParentNodeIsA) {
        const objectToSend = {
          event:'b2b',
          type: 'nav',
          value: 'element_click',
          label: 'main_nav',
          content: e.target.innerText.trim(),
          num: 'NA',
        }
        pushGA(objectToSend)
        if(ym) ym(46297389,  'reachGoal', 'events', { events: objectToSend})
      }
    }
    else if(e.target &&  e.target.nodeName == 'A' && e.target.closest('.promo-block')) {
      const objectToSend = {
      event:'b2b',
      type: 'nav',
      value: 'element_click',
      label: 'banner_nav',
      content: e.target.innerText.trim(),
      num: 'NA',
      }
      pushGA(objectToSend)
      if(ym) ym(46297389,  'reachGoal', 'events', { events: objectToSend})
    }
  })
} else {
  // RTRU-14912
  waitLoaded('.header.ready',h=>{
    if(!h)return
    // Menu-links
    document.querySelectorAll('.main-nav__link').forEach(l=>{
      if(l.href)l.addEventListener('click',e=>{
        const parentTitle=l.closest('[data-parent-link]')?.getAttribute('data-parent-link')
        const authorized=isRtkUser()
        const params={[`${window.RTK_SEGMENT}_main_nav`]:{}}
        const v=(authorized)?{[l.innerText]:authorized}:l.innerText
        params[`${window.RTK_SEGMENT}_main_nav`][_getGaClientId()]=parentTitle?{[parentTitle]:v}:v
        ymSafePush('params',params)
      })
    })
    // Menu promo-block
    // waitLoaded(promoBlockSelector, pb=>{
    //   if(!pb)return
    //   document.querySelectorAll(promoBlockSelector).forEach(l=>{
    //     if(l.href)l.addEventListener('click',e=>{
    //       const title=l.querySelector('.rt-header-menu-promo-block__title')?.innerText?.trim()||'Ошибка верстки, заголовок'
    //       promoBlockPush({title})
    //     })
    //   })
    // })
  })
}

if (window.RTK_HEADER) {
  // Deprecated, @TODO - move to SSR
  window.RTK_HEADER.linkClick=val=>{
    pushGA({
      event: RTK_SEGMENT,
      type: 'main_nav',
      value: val
    })
  }
  window.RTK_HEADER.bannerLinkClick=val=>{
    pushGA({
      event: RTK_SEGMENT,
      type: 'banner_nav',
      value: val.title
    })
    promoBlockPush(val)
  }
  window.RTK_HEADER.burgerLinkClick=val=>{
    const objectToSend = {
      event:RTK_SEGMENT,
      type:'nav',
      value:'element_click',
      label:'main_nav',
      content:val,
      num:'NA',
    }
    pushGA(objectToSend)
    ymSafePush('reachGoal','events',{events:objectToSend})
  }
  window.RTK_HEADER.confirmGeoNotificationOpened=()=>{
    ymSafePush('reachGoal','b2c_view_geo_popup_Y')
  }
}

// FAQ annotations, if not exists .faq-custom-push
waitLoaded('header', function () {
  waitLoaded('.rt-annotation', function (el) {
    if (!el) return;
    var elFaq = document.querySelector('#faq')
    if (!elFaq || elFaq.classList.contains('faq-custom-push')) return;
    var items = elFaq.parentElement.querySelectorAll('.rt-annotation__title');
    items && items.forEach(function (i) {
      i.addEventListener('click', function () {
        pushGA({
          event: window.RTK_SEGMENT,
          type: 'support_question',
          value: (i.textContent).trim()
        })
      })
    })
  })
})

// App-Stores links
waitLoaded('.web-component .rtk-footer__badges', function (el) {
  if (!el) return
  var map = [['.rtk-badge_google-play', 'google-play'], ['.rtk-badge_app-store', 'app-store']]
  map.forEach(function (data) {
    var links = document.querySelectorAll(data[0])
    if (!links) return
    links.forEach(function (el) {
      el.addEventListener('click', function () {
        pushGA({
          event: RTK_SEGMENT,
          type: 'store',
          value: data[1]
        })
      })
    })
  })
})
