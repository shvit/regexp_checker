<script>
let iteratorInitSetActiveLink = 0

let setActiveLink = function () {
    if (iteratorInitSetActiveLink > 30) {
        return
    }
    let menus = document.querySelectorAll('.is-menu')
    if (menus.length < 2) {
        iteratorInitSetActiveLink ++
        setTimeout(setActiveLink, 300)
        return
    }
    menus.forEach(function (el) { 
        RTK_UTILS.ui.activateDrupalLinks(el)
        let annotations = el.querySelectorAll('header .rt-annotation')

        if (!annotations) {
            iteratorInitSetActiveLink ++
            setTimeout(setActiveLink, 300)
            return
        }
        if (!annotations.length) { return }
        let activeAnnotations = Array.prototype.find.call(annotations, function (annotation) { return annotation.querySelector('.is-active') })
        
        if (activeAnnotations) {
            activeAnnotations.classList.add('rt-annotation--is-open')
        } else {
            iteratorInitSetActiveLink ++
            setTimeout(setActiveLink, 300)
            return
        }
    })
}


window.addEventListener('load', function() {
  setActiveLink()
  let tabsForCheckedSegment = document.querySelectorAll('header .rt-tabs-mobile .rt-tabs-navigation__item-name')
  Object.keys(tabsForCheckedSegment).forEach(function (id) {
    if (tabsForCheckedSegment[id]) {
      tabsForCheckedSegment[id].addEventListener('click', setActiveLink)
    }
  })
})
</script>

<style>

.header--panels  .rtk-tabs__item span {
  font-weight: normal;
}

</style></div>
      
  </div>
