<script type="text/javascript">
	// возвращает значение параметра по имени
	function getParameterByName (name, url) {
		if (!url) url = window.location.href;
		name = name.replace(/[\[\]]/g, "\\$&");
		var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
				results = regex.exec(url);
		if (!results) return null;
		if (!results[2]) return '';
		return decodeURIComponent(results[2].replace(/\+/g, " "));
	}

	function menuClick (url, title, blank) {
		if(!blank)blank=undefined;
		if(window.RTK_UTILS&&window.RTK_UTILS.ui&&typeof window.RTK_UTILS.ui.currentBP==='function'&&window.RTK_UTILS.ui.currentBP()==='md'&&url.indexOf('#')!==-1){
			window.RTK_TOGGLE_REGIONS_FN('');window.location.href=url;window.location.reload();
		}
		// if (blank) { window.open(url, '_blank'); } else { window.location.href = url; }
	}
