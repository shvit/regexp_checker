  var
    mainLoader=document.querySelector('.rtk-loader'),
    mainWrapper=document.querySelector('.main-wrapper');
  function hideBlock(el){if(el)el.style.display='none'};
  function showBlock(el){if(el)el.style.display='block'};
  showBlock(mainLoader);
  hideBlock(mainWrapper);
  /* Функции для сборки скриптов */
  function addStyle(link){
    var
      _style=document.createElement('link'),
      _container=document.head;
    _style.href=link;
    _style.rel="stylesheet";
    _style.type='text/css';
    _container.appendChild(_style);
    return _style;
  }
  function addScript(link,noDefer){
    var
      _script=document.createElement('script'),
      _container=document.body||document.head;
    _script.src=link;
    _script.type='text/javascript';
    if(!noDefer)_script.defer=1;
    _container.appendChild(_script);
    return _script
  }
  function createPreloadedLink(link, as){
    function _createLink(){return document.createElement('link')}
    if(window.preloadedLinksSupports === undefined)
      window.preloadedLinksSupports=_createLink().relList.supports('preload')||0
    if(window.preloadedLinksSupports) {
      var _link=_createLink();
      _link.rel='preload';
      _link.href=link;
      _link.as=as||'script';
      document.head.appendChild(_link);
    }
  }
  // createPreloadedLink(window.RTK_ASSETS_URL+'css/stylekit-styles.css', 'style');
  addStyle(window.RTK_ASSETS_URL+'css/stylekit-styles.css').onload=function(){
    showBlock(mainWrapper);
    hideBlock(mainLoader);
  };
