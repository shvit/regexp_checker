// Yandex.Metrika script options
const METRIC_OPTIONS = {
  clickmap: true,
  trackLinks: true,
  accurateTrackBounce: true,
  webvisor: true,
  ecommerce: 'dataLayer'
}

const METRIC_OPTIONS_FOR_NEW_RT = {
  clickmap: true,
  trackLinks: true,
  accurateTrackBounce: true,
  ecommerce: 'dataLayer'
}

// Yandex.Metrika ids counters
window.RTK_YANDEX_COUNTER_IDS = {
  NEW_RT: 46297389,
  B2B: 87747728,
  B2C: 87747669,
  B2O: 87747838,
  GAME: 87747880,
  LC: 50289415,
  ECO_VATS: 89936172
}

function isGamePage () {
  return ['gg', 'rtx', 'gfn'].some(function (host) { return window.location.hostname.startsWith(host); }) ||
      ['4game', 'games', 'gfn', 'igrovoy', 'rtx', 'vkplay', 'wargaming', 'lestagames'].some(function (path) { return window.location.pathname.indexOf('/' + path) !== -1; })
}

let currentSegmentCode
// Yandex.Metrika b2b
if (window.location.pathname.includes('/b2b')) {
  currentSegmentCode = window.RTK_YANDEX_COUNTER_IDS.B2B;
  // B2B-7060
  if (window.location.pathname.match(/(\/b2b\/telephony\/vats$)/) || window.location.pathname.includes('/b2b/internet/fix') || window.location.pathname.includes('/b2b/internet-100mb-lp1-price'))
    ym(window.RTK_YANDEX_COUNTER_IDS.ECO_VATS, 'init', METRIC_OPTIONS);
// Yandex.Metrika b2o
} else if (window.location.pathname.includes('/b2o') || window.location.pathname.includes('/volshebnye-skazki')) {
  currentSegmentCode = window.RTK_YANDEX_COUNTER_IDS.B2O;
  // Yandex.Metrika lc
} else if (window.location.pathname.includes('/lc')) {
  currentSegmentCode = window.RTK_YANDEX_COUNTER_IDS.LC;
  // Yandex.Metrika eco
} else if (window.location.pathname.includes('/eco')) {
  currentSegmentCode = window.RTK_YANDEX_COUNTER_IDS.ECO_VATS;
  ym(window.RTK_YANDEX_COUNTER_IDS.B2B, 'init', METRIC_OPTIONS);
} else {
  // Yandex.Metrika b2c
  currentSegmentCode = window.RTK_YANDEX_COUNTER_IDS.B2C;
}
// Yandex.Metrika all pages
ym(window.RTK_YANDEX_COUNTER_IDS.NEW_RT, 'init', METRIC_OPTIONS_FOR_NEW_RT);
// Yandex.Metrika segment
ym(currentSegmentCode, 'init', METRIC_OPTIONS);
// Yandex.Metrika gg
if (isGamePage()) {
  ym(window.RTK_YANDEX_COUNTER_IDS.GAME, 'init', METRIC_OPTIONS);
}</script>
